{
    "version": 2,
    "builds": [
      {
        "src": "next.config.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/static/streaming\\.proto",
        "dest": "/static/streaming.proto",
        "headers": {
          "Content-Type": "application/octet-stream"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/next.config.mjs"
      }
    ]
  }